<div class="row justify-content-center">
  <h1>
    Product Search
  </h1>
</div>

<div class="row">
  <div class="col-5">
    <div class="card border border-secondary p-1">
      <div class="card-header p-1">
        <%= render "product_search/form", search_set: @search_set, input_group: @input_group %>
      </div>
      <div class="card-body p-1">
        <%#= render 'product_items/aside', product_items: @product_items %>
      </div>
    </div>
  </div>

  <div class="col-7">
    <div id="show-card">
    </div>
  </div>
</div>
