  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">invoice #</th>
      </tr>
    </thead>

    <tbody>
      <%= render 'suppliers/table_body', invoices: @supplier.invoices %>
    </tbody>
  </table>
</div>
