<div class="row justify-content-center">
  <h1>Invoice List</h1>
</div>
<hr>

<table class="table table-striped table-hover">
  <thead>
    <th>Invoices Name</th>
  </thead>
  <tbody>
    <% @invoices.each do |invoice| %>
      <%= render 'invoices/table_row', invoice: invoice %>
    <% end %>
  </tbody>
</table>
